@import "~styles-var"; 
@import "~commond-var";
.content
	width  95% 
	margin  0 auto 
	pading  0 auto 
	position  relative 
	background-color  #F0F0F0 
.nav
	float left
	width 12%
	position absolute
	top 0
	bottom 0
	overflow auto
	.nav-menu
		color #fff
		font-size:24px
	.nav-logo
		default-padding-lr()
		height 60px
		line-height 60px
		font-size 22px
		font-weight bold
		color #fff
.form
	position relative
	width 88%
	left 12%
	overflow hidden
	.form-title,.form-term
		width 100%
		height 60px
		line-height 60px
		background-color #fff
		color black
	.form-nav
		default-commond()
		.el-breadcrumb
			line-height 56px
			font-size 16px
			default-margin-lr()
	.form-body
		.form-search
			default-commond()
			border-top 1px #E0E1E1 solid
			border-bottom 1px #E0E1E1 solid
			background-color #F1F1F1
			default-padding-lr()
		.form-term
			padding 0 40px
//重写 el style 
.el-menu
	background-color  black
	li
		background-color  #2B2E34		
		font-size 14px		
		color #fff
	.el-menu-item
		
		box-shadow 1px 1px 1px 0px #000
	.el-menu-item:first-child
		border-top 1px solid #000	
.el-submenu__title
	font-size 16px
	color #fff	
.el-form-item__label
	font-size 16px
.el-input
	display inline	
.el-input__inner
	width 400px
	display inline
	margin-left 10px
	